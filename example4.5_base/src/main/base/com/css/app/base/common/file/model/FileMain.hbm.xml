<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.css.app.base.common.file.model.FileMain" table="file_main">
 
        <id name="uuid" type="java.lang.String">
            <column name="uuid" length="32" />
             <generator class="assigned" />
        </id>
        <property name="fileMd5" type="java.lang.String">
            <column name="fileMd5" length="32" />
        </property>
        <property name="parentId" type="java.lang.String">
            <column name="parentId" length="32" />
        </property>
        <property name="name" type="java.lang.String">
            <column name="name" length="50" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="createTime" length="19" />
        </property>
        <property name="orderNum" type="java.lang.Integer">
            <column name="orderNum" length="10" />
        </property>
        <property name="category" type="java.lang.String">
            <column name="category" length="20" />
        </property>
        <property name="fileType" type="java.lang.String">
            <column name="fileType" length="250" />
        </property>
        <property name="fileExt" type="java.lang.String">
            <column name="fileExt" length="50" />
        </property>
        <property name="fileSize" type="java.lang.Long">
            <column name="fileSize" length="20" />
        </property>
        <property name="isFolder" type="java.lang.String">
            <column name="isFolder" length="10" />
        </property>
        <property name="userId" type="java.lang.String">
            <column name="userId" length="32" />
        </property>
        <property name="shareFromUserId" type="java.lang.String">
            <column name="shareFromUserId" length="32" />
        </property>
        <property name="publicFlag" type="java.lang.String">
            <column name="publicFlag" length="10" />
        </property>
        <property name="favourite" type="java.lang.String">
            <column name="favourite" length="10" />
        </property>

    </class>
</hibernate-mapping>
